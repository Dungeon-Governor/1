{>load}
<div class="form-group">
    <label for="{{panel_id}}_archetype">Archetype Id</label>
    <select id="{{panel_id}}_archetype" class="form-control" id="selectArchetypeId">
        {{#each archetypes}}
        <option value="{{archetypeId}}">{{archetypeId}}</option>
        {{/each}}
    </select>
</div>

{>specialize}
<div class="form-group">
    <label for="{{panel_id}}_parent">Parent Archetype Id</label>
    <select id="{{panel_id}}_parent" class="form-control" id="selectArchetypeId">
        {{#each archetypes}}
            <option value="{{archetypeId}}">{{archetypeId}}</option>
        {{/each}}
    </select>
</div>
<div class="form-group">
    <label for="{{panel_id}}_specialized">Specialized Archetype Id</label>
    <input id="{{panel_id}}_specialized" type="text" class="form-control input-sm">
</div>


